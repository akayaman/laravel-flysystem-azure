{
  "name": "akayaman/laravel-flysystem-azure",
  "description": "Laravel Azure storage service provider",
  "keywords": [
    "laravel",
    "flysystem",
    "azure"
  ],
  "authors": [
    {
      "name": "NAKAYAMA Atsushi",
      "email": "anaka@yo.rim.or.jp"
}
  ],
  "license": "MIT",
  "require": {
    "php": ">=5.6.4",
    "akayaman/flysystem-azure-adapter": "~1.0.0.4",
    "illuminate/support": "5.3.* || 5.4.* || 5.5.* || 5.6.*"
  },
  "autoload": {
    "psr-4": {
      "FutureOriented\\LaravelFlysystemAzure\\": "src/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "FutureOriented\\LaravelFlysystemAzure\\AzureStorageServiceProvider"
      ]
    }
  }
}
